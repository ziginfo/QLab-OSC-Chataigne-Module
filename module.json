{
	"name": "QLab OSC Advanced",
	"type": "OSC",
	"path": "Software",
	
	"version":"1.4",
  "description":"Allows you to control QLab with Chataigne",
  "url":"https://github.com/ziginfo/QLab-OSC-Chataigne-Module",
  "downloadURL":"https://github.com/ziginfo/QLab-OSC-Chataigne-Module/archive/refs/heads/main.zip",
	
	"hasInput": true,
	"hasOutput": true, 

	"hideDefaultCommands": false,
	"hideDefaultParameters": [
	"colorSendMode",
		"autoRange",
		"autoFeedback",
		"pass_through",
		"input",
		"outputs",
		"useHierarchy",
		"colorSendMode",
		"booleanSendMode",
		"listenToFeedback" ],
	
	"defaults": {
	
		"OSC Input": {
			"localPort" : 53001 },
			
		"OSC Outputs": {
			"OSC Output": {
				"local" : false,
				"remotePort" : 53000,
				"listenToFeedback":false } }
	},
	
	"parameters": {
			"Advice":
			{ "type": "String",
			"multiline" : true,
			"readOnly" : true , 
			"description":"After you've inserted a new QLab-Module or  loading a new QLab-Session (Workspace), please do a \"Sync\" first and than 'Save' the Session (cmd-S) and 'Reload' it again (cmd-shift-O) !",
			"default":"After you've inserted a new QLab-Module or when loading a new QLab-Session (Workspace), please do a \"Sync\" first and than 'Save' the Session (cmd-S) and 'Reload' it again (cmd-shift-O) !" },
				
			"Workspace ID":
			{
				"type":"String",
				"readOnly": true ,
				"default":"Workspace ID",
				"description":"Set the Workspace ID or Name" 
			},
			"Number of CueLists":
			{
				"type":"Integer",
				"description":"Count of CueLists"
			},
			"Number of Cues":
			{
				"type":"Integer",
				"description":"Count of Cues"
			},
			"Set Feedback Rate": {
				"type":"Enum",
				"description": "Select the Update Rate for constant Feedback",
				"options": {
				"medium (2sec)": 2,
				"once/min": 60,
            	"very slow (15sec)": 15,
            	"slow (5sec)": 5,            	
            	"normal (1sec)": 1,
            	"fast (0,5sec)": 0.5,
            	"faster (0.2sec)": 0.2,
            	"very fast (0,1sec)": 0.1 ,
            	"max speed ": 0  }  }
			
	},
	
	"scripts": ["qlab.js"],
	
	"values": {
			"Workspace ID": {
				"type":"String",
				"default":"Workspace ID",
				"description":"Set the Workspace ID or Name"  },
			"Playing Time": {
				"type":"Float",
				"readOnly": true, 
				"ui":"time"  },
			"Playing Cue": {
				"type":"String",
				"description":"Shows the Active Cue Name",
				"default":"Active Cue"  },
			"Next Cue": {
				"type":"String",
				"description":"Shows the Next Cue Name",
				"default":"Next Cue"  },
			"Sync": {
				"type":"Trigger",
				"description":"Sync Infos" },	
				
				
			"Cue Names": {
				"type":"Container",
			"Advice": {
				"type":"String",
				"default":"If no Cues showing, please Sync the Cue Name List.",
				"readOnly": true },
			"Sync Cue Names": {
				"type":"Trigger",
				"description":"Syncing Cue Names",
				"default": false },
			"Number of Cues": {
				"type":"Integer",
				"description":"Count of Cues" },
			"Count of CueLists": {
				"type":"Integer",
				"description":"Count of CueLists" },
			"Active CueList": {
				"type":"Integer",
				"description":"Shows Active CueLists ID" },
			"CueList Name": {
				"type":"String",
				"default":"Name of the active Cuelist",
				"description":"Name of the active Cuelist" },
			"   ": {
				"type":"String",
				"default":"Cue Names",
				"readOnly": true },	
			"Cue Name Shows": {
				"type":"Enum",
				"description": "Select what the Name-Field should show !",
				"default": 1 ,
				"options": {
				"Number-Name-Color": 1 ,
				"Name Only": 2 ,
                "Number Only": 3 ,
                "Color Only": 4,
                "default": 1   }  }
			
			}
			
			
	 },
	
	
	
	"commands": {
	
	"Show Playing" : {
			"menu" : "Main Control",
			"callback" : "show_playing"
		},
		
	"Go" : {
			"menu" : "Main Control",
			"callback" : "go"
		},
		
	"Pause" : {
			"menu" : "Main Control",
			"callback" : "pause"
		},	
		
	"Continue" : {
			"menu" : "Main Control",
			"callback" : "resume"
		},
		
	"Stop Cue" : {
			"menu" : "Main Control",
			"callback" : "stop_cue",
			"parameters" : {
			
			" Stop Cue No": {
					"type": "String",
					"useForMapping": true }						
		}  },	
		
	"Previous Cue" : {
			"menu" : "Main Control",
			"callback" : "back"
		},
		
	"Next Cue" : {
			"menu" : "Main Control",
			"callback" : "next"
		},
		
	"Stop Playlist" : {
			"menu" : "Main Control",
			"callback" : "stop_play"
		},			
		
	"Stop All" : {
			"menu" : "Main Control",
			"callback" : "stop_all"
		},
		
	"Stop in Time" : {
			"menu" : "Main Control",
			"callback" : "stop_intime",
			"parameters" : {
			
			" Load Cue No": {
					"type": "Float",
					"ui": "time" ,
					"useForMapping": true }						
		}
		},			
				
	"Load Cue" : {
			"menu" : "Main Control",
			"callback" : "set_next",
			"parameters" : {
			
			" Load Cue No": {
					"type": "String",
					"useForMapping": true }						
		}  },	
		
	"Start Cue" : {
			"menu" : "Main Control",
			"callback" : "start_next",
			"parameters" : {
			
			" Start Cue No": {
					"type": "String",
					"useForMapping": true }						
		}  },
		
	"Rename Cue" : {
			"menu" : "Cue Basics",
			"callback" : "rename_cue",
			"parameters" : {
			
			"Cue Number": {
					"type": "String",
					"useForMapping": true },
			"New Name": {
					"type": "String",
					"useForMapping": true }						
		}  },
		
	"Reumber Cue" : {
			"menu" : "Cue Basics",
			"callback" : "renumber_cue",
			"parameters" : {
			
			"Cue Number": {
					"type": "String",
					"useForMapping": true },
			"New Number": {
					"type": "String",
					"useForMapping": true }						
		}  },
		
	"Renumber All Cues" : {
			"menu" : "Cue Basics",
			"callback" : "renumber_allcues",
			"parameters" : {
			
			"Start Number": {
					"type": "Integer",
					"default": 1, 
					"min":1,
					"useForMapping": true },
					
			"Increment Number": {
					"type": "Integer",
					"default": 1, 
					"min":1,
					"useForMapping": true }					
		}  },
				
	"Reset Playlist" : {
			"menu" : "Main Control",
			"callback" : "reset"
		},
		
	"Prewait" : {
			"menu" : "Main Control",
			"callback" : "prewait_act",
			"parameters" : {
				
				"Set Pre Time": {
					"type": "Float",
					"ui":"time",
					"default": 0, 
					"min":0,
					"max":15,
					"useForMapping": true } 
			}  },
			
	"Postwait" : {
			"menu" : "Main Control",
			"callback" : "postwait_act",
			"parameters" : {
				
				"Set Post Time": {
					"type": "Float",
					"ui":"time",
					"default": 0, 
					"min":0,
					"max":15,
					"useForMapping": true } 
			}  },
		
	"Playing Cue" : {
			"menu" : "Cue Basics",
			"callback" : "active_cue"
		},
		
	"Selected Cue" : {
			"menu" : "Cue Basics",
			"callback" : "selected_cue"
		},
		
	"Playhead Cue" : {
			"menu" : "Cue Basics",
			"callback" : "playhead_cue"
		},
		
	"Active & Next Cue" : {
			"menu" : "Cue Basics",
			"callback" : "request_activecue"
		},
		
		
	"Volumes db" : {
			"menu" : "Volume",
			"callback" : "actVol_at",
			"parameters" : {
				
				"Volume db": {
					"type": "Float",
					"default": 0, 
					"min":-30,
					"max":12,
					"useForMapping": true } 
			}  },	
		
	"Volume 0db" : {
			"menu" : "Volume",
			"callback" : "actVol_nominal"
		},	
		
	"Volume Off" : {
			"menu" : "Volume",
			"callback" : "actVol_off"
		},		
		
	"Volume Full" : {
			"menu" : "Volume",
			"callback" : "actVol_full"
		},
		
	"Request Cue Names" : {
			"menu" : "Requests",
			"callback" : "request_names"
		},
		
	"Request Sel Cue" : {
			"menu" : "Requests",
			"callback" : "request_selcue"
		},
		
	"Request Cuelists" : {
			"menu" : "Requests",
			"callback" : "request_cuelists"
		},	
		
	"Request Unique IDs" : {
			"menu" : "Requests",
			"callback" : "request_uniqueids"
		},	
					
	"Reply Always" : {
			"menu" : "Requests",
			"callback" : "reply",
			"parameters" : {
				
				"ON/OFF": {
					"type":"Enum",
					"options": {
						"Start" : 1,
						"Stop" : 0 } }
			}  },			
		
	"Updates" : {
			"menu" : "Requests",
			"callback" : "updates",
			"parameters" : {
				
				"ON/OFF": {
					"type":"Enum",
					"options": {
						"Start" : 1,
						"Stop" : 0 } }
			}   }

	
	}
}
